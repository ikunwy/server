#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules/pkg/lib-es5/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules/pkg/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules/pkg/lib-es5/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules/pkg/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/pkg@5.8.1/node_modules:/mnt/c/Users/HP/Desktop/projectAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib-es5/bin.js" "$@"
else
  exec node  "$basedir/../../lib-es5/bin.js" "$@"
fi
